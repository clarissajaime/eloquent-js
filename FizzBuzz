//print num 1-100
for (var i = 1; i <= 100; i++) {
  //if num divisible by both 3 & 5, print "fizzbuzz"
  if ((i % 3 == 0) && (i % 5 == 0)) {
    console.log("fizzbuzz");
    //if num divisible by 3 print "fizz"
  } else if (i % 3 == 0) {
  	console.log("fizz");
  //if num divisible by 5 print "buzz"
  } else if (i % 5 == 0) {
    console.log("buzz");
  } else {
  //otherwise print the number
   console.log(i); 
  }
}
